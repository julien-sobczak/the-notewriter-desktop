  "noteTypes": {
    "Artwork": {
      "name": "Artwork",
      "pattern": "(?i)^Artwork:\\s*(.*)$",
      "processors": null,
      "hooks": null
    },
    "Flashcard": {
      "name": "Flashcard",
      "pattern": "(?i)^Flashcard:\\s*(.*)$",
      "processors": [
        "flashcard-extractor"
      ],
      "attributes": [
        {
          "name": "srs.algorithm",
          "required": false,
          "inline": false
        },
        {
          "name": "srs.boostFactor",
          "required": false,
          "inline": false
        }
      ],
      "hooks": null
    },
    "Generator": {
      "name": "Generator",
      "pattern": "(?i)^Generator:\\s*(.*)$",
      "processors": [
        "generator",
        "list-items"
      ],
      "attributes": [
        {
          "name": "file",
          "required": false,
          "inline": false
        },
        {
          "name": "interpreter",
          "required": false,
          "inline": false
        }
      ],
      "hooks": null
    },
    "Journal": {
      "name": "Journal",
      "pattern": "(?i)^Journal:\\s*(.*)$",
      "processors": [
        "date-extractor",
        "list-items"
      ],
      "attributes": [
        {
          "name": "date",
          "required": true,
          "inline": false
        }
      ],
      "hooks": null
    },
    "List": {
      "name": "List",
      "pattern": "(?i)^List:\\s*(.*)$",
      "processors": [
        "list-items"
      ],
      "hooks": null
    },
    "Master": {
      "name": "Master",
      "pattern": "(?i)^Master:\\s*(.*)$",
      "processors": null,
      "hooks": null
    },
    "Note": {
      "name": "Note",
      "pattern": "(?i)^Note:\\s*(.*)$",
      "processors": null,
      "hooks": null
    },
    "Quote": {
      "name": "Quote",
      "pattern": "(?i)^Quote:\\s*(.*)$",
      "processors": [
        "quote-rewriter"
      ],
      "hooks": null
    },
    "ReadingList": {
      "name": "ReadingList",
      "pattern": "(?i)^ReadingList:\\s*(.*)$",
      "processors": [
        "list-items"
      ],
      "attributes": [
        {
          "name": "author",
          "required": false,
          "inline": false
        },
        {
          "name": "rating",
          "required": false,
          "inline": false
        }
      ],
      "hooks": null
    },
    "Routine": {
      "name": "Routine",
      "pattern": "(?i)^Routine:\\s*(.*)$",
      "processors": null,
      "hooks": null
    },
    "Task": {
      "name": "Task",
      "pattern": "(?i)^Task:\\s*(.*)$",
      "processors": null,
      "attributes": [
        {
          "name": "status",
          "required": true,
          "inline": false
        },
        {
          "name": "due",
          "required": false,
          "inline": false
        },
        {
          "name": "priority",
          "required": false,
          "inline": false
        }
      ],
      "hooks": null
    },
    "Todo": {
      "name": "Todo",
      "pattern": "(?i)^Todo:\\s*(.*)$",
      "processors": [
        "list-items"
      ],
      "attributes": [
        {
          "name": "status",
          "required": true,
          "inline": true
        },
        {
          "name": "due",
          "required": false,
          "inline": true
        },
        {
          "name": "priority",
          "required": true,
          "inline": true
        }
      ],
      "hooks": null
